{
  "name": "bank-api-system",
  "version": "1.0.0",
  "description": "Bank API",
  "author": "Kevin Vogel",
  "npmClient": "pnpm",
  "scripts": {
    "build": "pnpm run '/build:*/'",
    "build:sdk": "pnpm --filter @bank/sdk build:all",
